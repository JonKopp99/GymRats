import UIKit
class workoutsTBCell: UITableViewCell {
@IBOutlet weak var name: UILabel!
@IBOutlet weak var theImage: UIImageView!
var complete: Bool!
var completeButton: UIButton!
override func layoutSubviews() {

theImage.frame = CGRect(x: 5, y: 5, width: frame.width * 0.3, height: frame.height-10)
theImage.contentMode = .scaleAspectFit
name.frame = CGRect(x: frame.width * 0.3 + 20, y: frame.height/2-20, width: frame.width/2, height: 40)

if(complete == nil)
{
complete = false
}

completeButton = UIButton(frame: CGRect(x: frame.width-50, y: frame.height/2-25, width: 50, height: 50))
if(complete)
{
completeButton.setImage(#imageLiteral(resourceName: "checkMark"), for: .normal)
}
else
{
completeButton.setImage(#imageLiteral(resourceName: "xMark"), for: .normal)
}
completeButton.backgroundColor = .clear
completeButton.addTarget(self, action:#selector(self.completeButtonPressed), for: .touchUpInside)
self.addSubview(completeButton)

}
@objc func completeButtonPressed()
{
completeButton.removeFromSuperview()
print("Tap Recognized")
if(complete)
{
complete = false

completeButton.setImage(#imageLiteral(resourceName: "xMark"), for: .normal)
}
else
{
completeButton.setImage(#imageLiteral(resourceName: "checkMark"), for: .normal)
complete = true
}
self.addSubview(completeButton)
}
}
